{
  "version": 7,
  "cmakeMinimumRequired": {
    "major": 3
  },
  "configurePresets": [
    {
      "name": "debug-env",
      "displayName": "debug-env",
      "hidden": true,
      "binaryDir": "${fileDir}/build/${presetName}",
      "environment": {
        "ARMGCC_DIR": "$env{ARM_GCC_DIR}",
        "SdkRootDirPath": "${sourceDir}/../..",
        "MCUX_VENV_PATH": "",
        "PATH": "$env{MCUX_VENV_PATH}${pathListSep}$penv{PATH}",
        "POSTPROCESS_UTILITY": "",
        "CMAKE_EXPORT_COMPILE_COMMANDS": "ON"
      },
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Debug",
        "CPM_SOURCE_CACHE": "${fileDir}/../../cpm"
      }
    },
    {
      "name": "release-env",
      "displayName": "release-env",
      "hidden": true,
      "binaryDir": "${fileDir}/build/${presetName}",
      "environment": {
        "ARMGCC_DIR": "$env{ARM_GCC_DIR}",
        "SdkRootDirPath": "${sourceDir}/../..",
        "MCUX_VENV_PATH": "",
        "PATH": "$env{MCUX_VENV_PATH}${pathListSep}$penv{PATH}",
        "POSTPROCESS_UTILITY": "",
        "CMAKE_EXPORT_COMPILE_COMMANDS": "ON"
      },
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "RelWithDebInfo",
        "CPM_SOURCE_CACHE": "${fileDir}/../../cpm"
      }
    }
  ],
  "buildPresets": []
}