From 822c91d2db087741ccc694793f96f5af6e3527f5 Mon Sep 17 00:00:00 2001
From: Karel Tucek <kareltucek@centrum.cz>
Date: Wed, 18 Feb 2026 12:33:00 +0100
Subject: [PATCH] suppress unused startTick warning

---
 device/usb_device_ehci.c | 2 +-
 device/usb_device_khci.c | 2 +-
 2 files changed, 2 insertions(+), 2 deletions(-)

diff --git a/device/usb_device_ehci.c b/device/usb_device_ehci.c
index 5315688..3061bba 100644
--- a/device/usb_device_ehci.c
+++ b/device/usb_device_ehci.c
@@ -1947,7 +1947,7 @@ usb_status_t USB_DeviceEhciControl(usb_device_controller_handle ehciHandle, usb_
 #endif
 #if (defined(USB_DEVICE_CONFIG_LOW_POWER_MODE) && (USB_DEVICE_CONFIG_LOW_POWER_MODE > 0U))
 #if ((defined(USB_DEVICE_CONFIG_REMOTE_WAKEUP)) && (USB_DEVICE_CONFIG_REMOTE_WAKEUP > 0U))
-    uint64_t startTick;
+    uint64_t startTick __attribute__((unused));
 #endif
 #endif
 
diff --git a/device/usb_device_khci.c b/device/usb_device_khci.c
index f0e1fce..0782be2 100644
--- a/device/usb_device_khci.c
+++ b/device/usb_device_khci.c
@@ -1510,7 +1510,7 @@ usb_status_t USB_DeviceKhciControl(usb_device_controller_handle khciHandle, usb_
 #endif
 #if ((defined(USB_DEVICE_CONFIG_LOW_POWER_MODE)) && (USB_DEVICE_CONFIG_LOW_POWER_MODE > 0U))
 #if ((defined(USB_DEVICE_CONFIG_REMOTE_WAKEUP)) && (USB_DEVICE_CONFIG_REMOTE_WAKEUP > 0U))
-    uint64_t startTick;
+    uint64_t startTick __attribute__((unused));
 #endif
 #endif
     usb_status_t status = kStatus_USB_Error;
-- 
